{
    "name":"mooti/container",
    "description": "simple service container",
    "keywords": ["container", "services", "service container"],
    "type": "library",
    "license": "Apache-2.0",
    "require": {
        "php": ">=5.5.9",
        "icanboogie/inflector": "^1.3",
        "container-interop/container-interop": "^1.1",
        "mooti/factory": "^0.0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "5.1.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {"Mooti\\Container\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"Mooti\\Test\\PHPUnit\\Container\\": "tests/phpunit/src"}
    }
}
